{
    "preset": "laravel",
    "rules": {
        "no_superfluous_phpdoc_tags": false,
        "phpdoc_to_comment": false,
        "phpdoc_separation": false,
        "phpdoc_align": {
            "align": "left"
        },
        "phpdoc_trim_consecutive_blank_line_separation": true,
        "no_empty_phpdoc": false,
        "phpdoc_no_empty_return": false
    }
}
